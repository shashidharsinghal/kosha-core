{
  "name": "kosha-backend",
  "version": "1.0.0",
  "description": "Kosha Personal Finance App Backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docker:build": "docker build -t kosha-backend .",
    "docker:run": "docker run -p 3000:3000 --env-file .env kosha-backend",
    "docker:compose:up": "docker compose up -d",
    "docker:compose:down": "docker compose down",
    "docker:compose:logs": "docker compose logs -f backend",
    "docker:compose:ps": "docker compose ps",
    "docker:compose:restart": "docker compose restart"
  },
  "keywords": ["finance", "personal-finance", "kosha"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17",
    "redis": "^4.6.8",
    "zod": "^3.22.4",
    "winston": "^3.10.0",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "swagger-ui-express": "^5.0.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "prettier": "^3.0.2",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.0",
    "@types/js-yaml": "^4.0.9"
  }
}

